{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",

  "browser_action": {
    "default_title": "Hello, world"
  },
  "content_scripts": [
     {
     "matches": ["<all_urls>"],
     "js": ["jquery-3.1.1.min.js", "content.js"]
   }
 ],

 "background": {
   "scripts": ["background.js"],
   "persistent" : false
 }
}
